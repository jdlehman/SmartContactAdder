package edu.wm.weightedNames;

import java.util.Comparator;

/**
 * Class Name: WeightedNameComparator
 * Responsibilities: interface for WeightedName comparators
 * Collaborators: ComparatorByName, ComparatorByWeight
 * 
 * @author jonathanlehman
 *
 */

public interface WeightedNameComparator extends Comparator<WeightedName>{

	/** compares two WeightedNames by name
	 * @return int returns 1 if wn1 > wn2, -1 if wn1 < wn2 & 0 if they are equal
	 */
	public int compare(WeightedName wn1, WeightedName wn2);
	
}
